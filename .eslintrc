{
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    }
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/display-name": [
      1,
      {
        "ignoreTranspilerName": false
      }
    ],
    "react/jsx-uses-react": [
      1
    ],
    "react/wrap-multilines": [
      1
    ],
    "react/sort-comp": [
      1
    ],
    "react/react-in-jsx-scope": [
      1
    ],
    "react/prop-types": [
      1
    ],
    "react/no-direct-mutation-state": [
      2
    ],
    "react/no-did-update-set-state": [
      1,
      "allow-in-func"
    ],
    "react/no-deprecated": [
      1
    ],
    "react/jsx-uses-vars": [
      2
    ],
    "jsx-quotes": [
      1
    ],
    "no-console": [
      0
    ],
    "comma-style": [
      1,
      "last"
    ],
    "yoda": [
      1,
      "never"
    ],
    "dot-location": [
      1,
      "property"
    ],
    "no-void": [
      1
    ],
    "space-before-function-paren": [
      1,
      "always"
    ],
    "one-var": [
      1,
      {
        "uninitialized": "always",
        "initialized": "never"
      }
    ],
    "arrow-parens": [
      1,
      "always"
    ],
    "arrow-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ],
    "space-before-blocks": [
      1,
      "always"
    ],
    "array-bracket-spacing": [
      1,
      "always",
      {
        "singleValue": false,
        "objectsInArrays": false,
        "arraysInArrays": false
      }
    ],
    "object-curly-spacing": [
      1,
      "always",
      {
        "objectsInObjects": false,
        "arraysInObjects": false
      }
    ],
    "computed-property-spacing": [
      1,
      "never"
    ],
    "camelcase": [
      1,
      {
        "properties": "never"
      }
    ],
    "curly": [
      1,
      "multi-line"
    ],
    "no-multi-spaces": [
      1
    ],
    "space-in-parens": [
      1,
      "never"
    ],
    "generator-star-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "eol-last": [
      1
    ],
    "no-trailing-spaces": [
      1
    ],
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-mixed-spaces-and-tabs": [
      1
    ],
    "comma-dangle": [
      0
    ],
    "object-shorthand": [
      1,
      "always"
    ],
    "prefer-template": [
      1
    ],
    "quotes": [
      1,
      "double"
    ],
    "semi": [
      1,
      "never"
    ],
    "no-implicit-coercion": [
      2
    ],
    "no-param-reassign": [
      2,
      {
        "props": true
      }
    ],
    "radix": [
      2
    ],
    "eqeqeq": [
      2
    ],
    "no-const-assign": [
      2
    ],
    "prefer-const": [
      1
    ],
    "prefer-reflect": [
      1,
      {
        "exceptions": [
          "apply"
        ]
      }
    ],
    "prefer-arrow-callback": [
      1
    ],
    "no-var": [
      1
    ],
    "default-case": [
      2
    ],
    "no-return-assign": [
      2,
      "always"
    ],
    "no-undef-init": [
      1
    ],
    "no-lonely-if": [
      2
    ],
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "strict": [
      1,
      "never"
    ],
    "new-cap": [
      2,
      {
        "newIsCap": true,
        "capIsNew": false
      }
    ]
  }
}
